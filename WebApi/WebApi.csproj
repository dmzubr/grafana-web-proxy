<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>GrafanaProxy.WebApi</RootNamespace>
    <AssemblyName>GrafanaProxy.WebApi</AssemblyName>
    <Authors>dmzubr</Authors>
    <Product>Grafana Web Proxy</Product>
    <Description>Grafana Web Proxy</Description>
    <PackageProjectUrl>https://github.com/dmzubr/grafana-web-proxy</PackageProjectUrl>
